<%#= link_to 'New Restaurant', new_restaurant_path(@restaurant), class: "btn btn-primary" %>
<h2> List of Apartments <%= Flat.count %> </h2>
<ul>
  <% @apartment.each do |restaurant| %>
    <li><%#= link_to restaurant.name, restaurant_path(restaurant) %></li> 
    <p> City: <%= restaurant.address %></p>
    <p> City: <%= restaurant.description %></p>
    <p> City: <%= restaurant.price_per_night %></p>
    <p> City: <%= restaurant.number_of_guests %></p>
  <% end %>
</ul>